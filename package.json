{
  "name": "react-oldschool",
  "version": "1.0.0",
  "description": "A simple boilerplate for React projects.",
  "scripts": {
    "dev": "yarn run clean:dev && run-p css:dev js:dev server:dev",
    "clean:dev": "node tasks/cleanup-dev",
    "css:dev": "node tasks/css-dev",
    "js:dev": "node tasks/js-dev",
    "server:dev": "browser-refresh server/www.js",
    "build": "node tasks/build"
  },

  "author": "AJ Meyghani",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "browser-refresh": "^1.7.3",
    "browser-refresh-client": "^1.1.4",
    "chokidar": "^2.0.4",
    "clean-css-cli": "^4.2.1",
    "ejs": "^2.6.1",
    "eslint": "^5.3.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "node-gzip": "^1.1.2",
    "npm-run-all": "^4.1.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"

    "browser-refresh-client": "^1.1.4",
    "chokidar": "^2.0.4",
    "clean-css-cli": "^4.2.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-static-gzip": "^1.1.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.2",
